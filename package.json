    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
