    "follow-redirects": "^1.15.0",
    "html-to-text": "^8.2.0",
    "js-yaml": "^3.14.0",
    "mysql": "^2.18.1",
    "@surgefm/nestjs-algolia": "^4.0.0",
    "parse-domain": "^5.0.0",
    "pg": "^8.7.3",
    "puppeteer": "^14",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rss-parser": "^3.12.0",
    "rxjs": "^7.5.5",
    "sequelize": "^6.19.0",
    "sequelize-typescript": "^2.1.3"
