{
  "name": "v2land-diamond-hoe-refreshed",
  "version": "0.0.1",
  "description": "钻石锄，浪潮新闻采集服务 News Crawling Service for V2Land 💎",
  "author": "oppilate <oppilate@tutanota.com> (https://github.com/oppilate)",
  "private": true,
  "license": "(MIT or GPL-3.0)",
  "repository": "github:oppilate/v2land-diamond-hoe-refreshed",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "//": "higher versions require ESM which is not compatible: https://github.com/peerigon/parse-domain/issues/145",
    "@elastic/elasticsearch": "^8",
    "@hapi/joi": "^17.1.1",
    "@nestjs/axios": "^0.0.8",
    "@nestjs/bull": "^0.5.5",
    "@nestjs/common": "^8.4.4",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.4",
    "@nestjs/elasticsearch": "^8.1.0",
    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/schedule": "^1.1.0",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/terminus": "^8.0.6",
    "@postlight/mercury-parser": "^2.2.1",
    "@v2land/mercury-parser": "^2.2.7",
    "bull": "^4.8.2",
    "class-transformer": "^0.5.1",
